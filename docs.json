{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Galtea Docs",
  "colors": {
    "primary": "#5286FF",
    "light": "#A9E3FE",
    "dark": "#171717"
  },
  "favicon": "/favicon.png",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "styling": {
    "eyebrows": "breadcrumbs",
    "codeblocks": "system"
  },
  "icons": {
    "library": "fontawesome"
  },
  "fonts": {
    "family": "Geist"
  },
  "appearance": {
    "default": "system",
    "strict": false
  },
  "background": {
    "color": {
      "light": "#f6f6f5",
      "dark": "#09090a"
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@galtea.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://platform.galtea.ai"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"]
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Blog",
          "href": "https://galtea.ai/blog/index.html",
          "icon": "newspaper"
        },
        {
          "anchor": "Changelog",
          "href": "https://docs.galtea.ai/changelog",
          "icon": "list-check"
        },
        {
          "anchor": "Migration To V3",
          "href": "https://docs.galtea.ai/migration-to-v3",
          "icon": "right-left"
        }
      ]
    },
    "tabs": [
